<h1>Listing bar_items</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Bar menu item</th>
      <th>Bar closeout</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bar_items.each do |bar_item| %>
      <tr>
        <td><%= bar_item.ticket_id %></td>
        <td><%= bar_item.bar_menu_item_id %></td>
        <td><%= bar_item.bar_closeout %></td>
        <td><%= link_to 'Show', bar_item %></td>
        <td><%= link_to 'Edit', edit_bar_item_path(bar_item) %></td>
        <td><%= link_to 'Destroy', bar_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar item', new_bar_item_path %>
